  <connectionStrings>
    <add name="TeamEntities" connectionString="metadata=res://*/TeamBinsData.csdl|res://*/TeamBinsData.ssdl|res://*/TeamBinsData.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=SHYJUDEVPC;initial catalog=Team;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" />
    
  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="2.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="PreserveLoginUrl" value="true" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="emailIdFrom" value="putyouremailAddressHere" />
    <add key="bccTrackingEmail" value="putyouremailAddressHere" />
    <add key="traceToEmail" value="putyouremailAddressHere" />    
